{
  "name": "@goldstack/template-management-cli",
  "version": "0.1.0",
  "license": "",
  "author": "",
  "sideEffects": false,
  "main": "dist/src/image.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:pack": "yarn compile && yarn webpack",
    "clean": "yarn docker:clean",
    "cli": "yarn node ./bin/template-management-cli",
    "cli:deploy-templates-dev:backend": "yarn cli deploy-set --set backend --repo goldstack-dev --deployment dev --emailResultsTo mxrogm@gmail.com --workDir tmp --skipTests false",
    "cli:schedule-deploy-templates-dev": "yarn cli schedule-all-deploy-sets --repo goldstack-dev --deployment dev",
    "compile": "tsc -p tsconfig.json",
    "compile:watch": "tsc -p tsconfig.json --watch",
    "compile:watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "yarn build && yarn docker:build $@ && template-docker-image-aws deploy $@",
    "docker:build": "node ./dist/src/build.js build $@",
    "docker:clean": "node ./dist/src/build.js clean $@",
    "infra": "template-docker-image-aws infra",
    "prepublishOnly": "yarn run build",
    "template": "template-docker-image-aws",
    "test:ci": "jest --passWithNoTests --config=./jest.config.js"
  },
  "dependencies": {
    "@goldstack/goldstack-email-send": "0.1.0",
    "@goldstack/infra-aws": "0.3.7",
    "@goldstack/project-build": "0.1.0",
    "@goldstack/project-install": "0.1.0",
    "@goldstack/template-build-set": "0.1.0",
    "@goldstack/template-repository": "0.1.0",
    "@goldstack/template-repository-bucket": "0.1.0",
    "@goldstack/template-s3": "0.3.34",
    "@goldstack/template-static-website-aws": "0.3.34",
    "@goldstack/utils-cli": "0.2.2",
    "@goldstack/utils-config": "0.3.5",
    "@goldstack/utils-git": "0.1.23",
    "@goldstack/utils-package": "0.3.6",
    "@goldstack/utils-project": "0.3.5",
    "mock-aws-s3": "^4.0.1",
    "source-map-support": "^0.5.19",
    "tmp": "^0.2.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@goldstack/template-docker-image-aws": "0.3.17",
    "@goldstack/utils-sh": "0.4.5",
    "@types/jest": "^26.0.21",
    "@types/mock-aws-s3": "^2.6.1",
    "@types/node": "^14.0.6",
    "@types/source-map-support": "^0.5.2",
    "@types/wtfnode": "^0.7.0",
    "@types/yargs": "^15.0.5",
    "aws-sdk": "2.721.0",
    "concurrently": "^5.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "pnp-webpack-plugin": "^1.6.4",
    "reload": "^3.1.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  }
}
