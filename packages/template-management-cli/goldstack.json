  "name": "template-management-cli",
  "template": "docker-image-aws",
  "templateVersion": "0.1.0",
  "configuration": {
    "imageTag": "template-management-cli"
  },
  "deployments": [
    {
      "name": "dev",
      "awsRegion": "us-west-2",
