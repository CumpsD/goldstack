    "@goldstack/template-repository-bucket": "0.1.0",
    "@goldstack/utils-package": "0.3.6",
    "@goldstack/utils-project": "0.3.5",
    "@goldstack/utils-sh": "0.4.5",
    "@goldstack/utils-template": "0.3.5",
