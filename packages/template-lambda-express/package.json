{
  "name": "@goldstack/template-lambda-express",
  "version": "0.3.1",
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "dist/templateLambdaExpress.js",
  "bin": {
    "template": "./bin/template",
    "template-lambda-express": "./bin/template"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:watch": "yarn clean && yarn compile:watch",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "compile:watch": "tsc -p tsconfig.json --watch",
    "compile:watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "generate:docs": "utils-docs-cli generate-template-docs modules/lambda-express/ ./docs/ && cp ./docs/* ../../../templates/packages/lambda-express/docs",
    "generate:schema": "utils-package-config-generate && cp schemas/* ../../../templates/packages/lambda-express/schemas",
    "prepublishOnly": "yarn run build",
    "publish": "utils-git changed --exec \"yarn npm publish $@\"",
    "test:ci": "jest --passWithNoTests --config=./jest.config.js",
    "version:apply": "utils-git changed --exec \"yarn version $@ && yarn version apply\"",
    "version:apply:force": "yarn version $@ && yarn version apply"
  },
  "dependencies": {
    "@goldstack/infra": "0.3.0",
    "@goldstack/infra-aws": "0.3.0",
    "@goldstack/utils-aws-cli": "0.3.0",
    "@goldstack/utils-cli": "0.2.0",
    "@goldstack/utils-config": "0.3.0",
    "@goldstack/utils-docker": "0.3.0",
    "@goldstack/utils-log": "0.2.0",
    "@goldstack/utils-package": "0.3.0",
    "@goldstack/utils-package-config": "0.3.0",
    "@goldstack/utils-s3-deployment": "0.3.0",
    "@goldstack/utils-sh": "0.4.0",
    "@goldstack/utils-template": "0.3.0",
    "@goldstack/utils-terraform": "0.3.0",
    "@goldstack/utils-terraform-aws": "0.3.0",
    "archiver": "^5.0.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@goldstack/utils-docs-cli": "0.3.0",
    "@goldstack/utils-git": "0.1.21",
    "@goldstack/utils-package-config-generate": "0.2.0",
    "@types/archiver": "^3.1.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/yargs": "^15.0.5",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-json-schema-generator": "^0.70.2",
    "typescript": "^3.9.5"
  }
}
