{
  "name": "@goldstack/utils-template-test",
  "version": "0.1.0",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "dist/utilsTemplateTest.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:watch": "yarn clean && yarn compile:watch",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "compile:watch": "tsc -p tsconfig.json --watch",
    "compile:watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "prepublishOnly": "yarn run build",
    "test": "jest --config=./jest.config.js --watch",
    "test:ci": "jest --passWithNoTests --config=./jest.config.js"
  },
  "dependencies": {
    "@goldstack/infra": "0.3.6",
    "@goldstack/template-build": "0.1.0",
    "@goldstack/template-docker-image-aws": "0.3.17",
    "@goldstack/template-email-send": "0.3.39",
    "@goldstack/template-lambda-express": "0.3.41",
    "@goldstack/template-nextjs": "0.3.35",
    "@goldstack/template-repository": "0.1.0",
    "@goldstack/template-s3": "0.3.34",
    "@goldstack/template-static-website-aws": "0.3.34",
    "@goldstack/utils-config": "0.3.5",
    "@goldstack/utils-package": "0.3.6",
    "@goldstack/utils-sh": "0.4.5",
    "@goldstack/utils-template": "0.3.5",
    "@goldstack/utils-yarn": "0.3.7",
    "axios": "^0.19.2",
    "extract-zip": "^2.0.1",
    "mock-aws-s3": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/mock-aws-s3": "^2.6.1",
    "@types/node": "^14.0.6",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^4.1.3"
  }
}
