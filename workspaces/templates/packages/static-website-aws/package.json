{
  "name": "@goldstack/static-website-aws",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist ./webDist",
    "compile": "tsc --build",
    "compile-watch": "tsc --build --watch",
    "compile-watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "compileOld": "tsc -p tsconfig.json",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "yarn web:build $@ && template deploy $@",
    "infra": "template infra",
    "prepublishOnly": "yarn run build",
    "template": "template",
    "template-ts": "ts-node scripts/template.ts",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js",
    "watch": "yarn compile && yarn web:build local && concurrently \"yarn web:watch local\" \"reload --dir ./webDist --watch-dir ./webDist --port 8076 --browser -v\" ",
    "watch:new": "yarn compile && yarn web:build local && concurrently \"yarn web:watch local\" \"light-server --serve ./webDist --watchexp ./webDist/* --port 8077 --delay 500 --open\" ",
    "web:build": "node ./dist/build.js $@",
    "web:watch": "node ./dist/watch.js $@"
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-replace": "^1.0.0"
  },
  "devDependencies": {
    "@goldstack/template-static-website-aws": "0.3.50",
    "@types/gulp": "^4.0.7",
    "@types/gulp-changed": "^0.0.33",
    "@types/gulp-replace": "^0.0.31",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "concurrently": "^5.2.0",
    "jest": "^26.6.3",
    "light-server": "^2.9.1",
    "nodemon": "^2.0.4",
    "reload": "^3.2.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  }
}
