{
  "$schema": "./schemas/package.schema.json",
  "name": "lambda-api-template",
  "template": "lambda-api",
  "templateVersion": "0.1.0",
  "configuration": {},
  "deployments": [
    {
      "name": "prod",
      "awsRegion": "us-west-2",
      "awsUser": "goldstack-dev",
      "configuration": {
        "lambdaNamePrefix": "goldstack-test-lambda-api",
        "apiDomain": "lambda-api.examples.templates.dev.goldstack.party",
        "hostedZoneDomain": "dev.goldstack.party",
        "cors": "https://app-nextjs-bootstrap.templates.dev.goldstack.party",
        "lambdas": {
          "$default0": {
            "function_name": "goldstack-test-lambda-api-__default",
            "route": "$default"
          },
          "ANY /admin/{proxy+}1": {
            "function_name": "goldstack-test-lambda-api-admin-_proxy__",
            "route": "ANY /admin/{proxy+}"
          },
          "ANY /cart/{sessionId}/items2": {
            "function_name": "goldstack-test-lambda-api-cart-_sessionId_-items",
            "route": "ANY /cart/{sessionId}/items"
          },
          "ANY /echo3": {
            "function_name": "goldstack-test-lambda-api-echo",
            "route": "ANY /echo"
          },
          "ANY /order/{id}4": {
            "function_name": "goldstack-test-lambda-api-order-_id_",
            "route": "ANY /order/{id}"
          },
          "ANY /user5": {
            "function_name": "goldstack-test-lambda-api-user-__index",
            "route": "ANY /user"
          },
          "ANY /user/{userId}6": {
            "function_name": "goldstack-test-lambda-api-user-_userId_",
            "route": "ANY /user/{userId}"
          }
        }
      },
      "tfStateKey": "lambda-api-template-prod-fbec3992bfc4f00aa381.tfstate"
    }
  ]
}