{
  "name": "@goldstack/docker-image-aws",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "Max Rohde",
  "sideEffects": false,
  "main": "src/image.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile && yarn docker:build $@",
    "clean": "yarn docker:clean",
    "compile": "tsc -p tsconfig.json",
    "compile-watch": "tsc -p tsconfig.json --watch",
    "compile-watch:light": "nodemon --watch ./src/ -e '*' --exec 'yarn compile'",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "deploy": "yarn docker:build $@ && template deploy $@",
    "docker:build": "node ./dist/src/build.js build $@",
    "docker:clean": "node ./dist/src/build.js clean $@",
    "image": "template image",
    "infra": "template infra",
    "prepublishOnly": "yarn run build",
    "template": "template",
    "test-ci": "jest --passWithNoTests --config=./jest.config.js"
  },
  "dependencies": {
    "@goldstack/template-docker-image-aws": "0.3.32",
    "@goldstack/utils-git": "0.1.27",
    "@goldstack/utils-sh": "0.4.14"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "concurrently": "^5.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.3"
  }
}
